# -*- mode: shell-script -*-

if [ -f ./devenv.local ]; then
    . ./devenv.local

    export DEV_VERSION
    export DEV_PACKAGE="${DEV_ROOT}/neo4j-community-${DEV_VERSION}-unix.tar.gz"
    export DEV_SHA=$(sha256sum ${DEV_PACKAGE} | cut --delimiter=' ' --fields=1)

    if ! which make >/dev/null; then
        sudo apt-get install make
    fi
else
    echo "Couldn't find ./devenv.local"
fi
